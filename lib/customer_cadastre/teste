String validarCampos()
    if (emailController.text.isEmpty) {
      infoGeneric = "- O campo Email obrigat처rio.";
    }
    if (senhaController.text.isEmpty) {
      infoGeneric = "- O campo Senha obrigat처rio.";
    }
    if (cpfController.text.isEmpty) {
      infoGeneric = "- O campo CPF n찾o deve estar vazio";
    }
    if (emailController.text.trim() == "@") {
      infoGeneric = "- O campo email n찾o pode conter apenas o \"@\".\n";
    }
    if (!RegExp(r'^[a-zA-Z0-9]+(@[a-zA-Z])').hasMatch(emailController.text)) {
      infoGeneric = "- O campo email deve conter \"@\".\n";
    } else {
      infoGeneric = "Cadastrado com sucesso";
    }
    return infoGeneric;